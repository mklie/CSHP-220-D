<Window x:Class="Assignment_4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment_4"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" WindowState="Maximized">
    <StackPanel Background="#FFD6C3C3">
        <TextBlock>Enter ZipCode:</TextBlock>
        <TextBox x:Name="uxZipCode" Background="#FFF0F0E4" TextChanged="uxZipCode_TextChanged"/>
        <Button x:Name="uxSubmit" Background="#FFF07373" IsEnabled="False">Submit</Button>
        
    </StackPanel>
</Window>
